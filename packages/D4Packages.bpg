MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = o407_r40.bpl o407_d40.bpl O407br40.bpl o407bd40.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------


o407_r40.bpl: o407_r40.dpk
  $(DCC)

o407_d40.bpl: o407_d40.dpk
  $(DCC)

O407br40.bpl: O407br40.dpk
  $(DCC)

o407bd40.bpl: o407bd40.dpk
  $(DCC)


